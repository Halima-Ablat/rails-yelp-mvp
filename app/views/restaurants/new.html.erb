<div class="container">
<br/>
<h2 class="text-center">Add a new restaurant</h2>
<br/>
<br/>
<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.input :phone_number %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
</div>
